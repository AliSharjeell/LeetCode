class Solution {
public:
    bool squareIsWhite(string coordinates) {
        if((coordinates[0]-'b')%2!=0&&(coordinates[1]-'0')%2!=0){
            return false;
        }
        else if((coordinates[0]-'b')%2==0&&(coordinates[1]-'0')%2==0){
            return false;
        }
        return true;
    }
};
